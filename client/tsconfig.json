{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "baseUrl": ".",
    "paths": {
      "@pages/*": ["src/pages/*"],
      "@components/*": ["src/components/*"],
      "@components": ["src/components"],
      "@styles/*": ["src/styles/*"],
      "@styles": ["src/styles"],
      "@hooks/*": ["src/hooks/*"],
      "@hooks": ["src/hooks"],
      "@constants": ["src/constants"],
      "@constants/*": ["src/constants/*"],
      "@stores": ["src/stores"],
      "@service": ["src/service"],
      "@libs/*": ["src/libs/*"],
      "@utils": ["src/utils"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"],
  "exclude": [
    "node_modules"
  ]
}